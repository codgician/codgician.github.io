# Git LFS tracking for images in content
content/**/*.jpg filter=lfs diff=lfs merge=lfs -text
content/**/*.jpeg filter=lfs diff=lfs merge=lfs -text
content/**/*.png filter=lfs diff=lfs merge=lfs -text
content/**/*.gif filter=lfs diff=lfs merge=lfs -text
content/**/*.webp filter=lfs diff=lfs merge=lfs -text
content/**/*.svg filter=lfs diff=lfs merge=lfs -text
